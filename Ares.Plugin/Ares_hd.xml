<window>
  <id>7294</id>
  <allowoverlay>no</allowoverlay>
  <defaultcontrol>2</defaultcontrol>

  <controls>

    <control>
      <description>BackGround</description>
      <type>image</type>
      <id>31</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1920</width>
      <height>1280</height>
      <texture>background.png</texture>
    </control>

    <control>
      <description>Ares Logo</description>
      <type>image</type>
      <id>32</id>
      <posX>60</posX>
      <posY>50</posY>
      <texture>Ares.png</texture>
    </control>

    <control>
      <description>Title Label</description>
      <type>label</type>
      <id>1</id>
      <posX>120</posX>
      <posY>50</posY>
      <label>Ares Player</label>
      <align>left</align>
      <textcolor>ffffffff</textcolor>
    </control>

    <control>
      <description>Stop</description>
      <type>button</type>
      <id>2</id>
      <posX>60</posX>
      <posY>125</posY>
      <label>Stop All</label>
      <onleft>2</onleft>
      <onright>3</onright>
      <onup>2</onup>
      <ondown>6</ondown>
    </control>

    <control>
      <description>Previous Music Title</description>
      <type>button</type>
      <id>3</id>
      <posX>560</posX>
      <posY>125</posY>
      <label>Previous</label>
      <onleft>2</onleft>
      <onright>4</onright>
      <onup>3</onup>
      <ondown>6</ondown>
    </control>

    <control>
